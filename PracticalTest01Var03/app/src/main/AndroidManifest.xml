    package="practicaltest01var03.eim.systems.cs.pub.ro.practicaltest01var03">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/contacts_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PracticalTest01Var03MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PracticalTest01Var03SecondaryActivity"
                    android:label="Secondary Action">
            <intent-filter>
                <action android:name=".PracticalTest01SecondaryActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>
    </application>

</manifest>